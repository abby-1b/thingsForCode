{
    "name": "[...]",
    "description": "I don't want copyright infringement.",
    "version": "1.0.1",
    "manifest_version": 2,
    "permissions": ["activeTab", "storage", "alarms", "https://raw.githubusercontent.com/CodeIGuess/thingsForCode/master/ExtensionEDS/*"],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "pg.html",
        "default_title": "[...]"
    },
    "background": {
        "scripts": ["bg.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "content_security_policy": "script-src 'self' https://www.google-analytics.com; script-src 'self' 'unsafe-eval'; object-src 'self'",
        "js": ["dl.js"],
        "run_at": "document_end"
    }]
}