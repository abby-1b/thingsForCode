{
    "name": "[...]",
    "description": "I don't want copyright infringement.",
    "version": "1.0.1",
    "manifest_version": 3,
    "permissions": ["activeTab", "storage", "scripting"],
    "host_permissions": [
    	"https://raw.githubusercontent.com/CodeIGuess/thingsForCode/master/ExtensionEDS/*"
  	],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "background": {
        "service_worker": "bg.js"
    },
    "content_security_policy": {
        "script-src": "self"
    },
    "action": {
        "default_icon": "icon.png",
        "default_popup": "pg.html",
        "default_title": "[...]"
    },
    "content_scripts": [{
        "matches": [ "<all_urls>" ],
        "js": ["dl.js"],
        "all_frames": true
    }]
}